<?xml version="1.0" encoding="ISO-8859-1"?>

<objectDefinitions>

<objectDef name="tails" type="RadiusBlock">
	<animations default="flying">
		<animation name="flying">
			<frames>
				<frame name="fly1.png" duration="4" />
				<frame name="fly2.png" duration="4" />
			</frames>
			<show_in_order />
			<repeat />
		</animation>
	</animations>
	<properties>
		<theta type="random" min="0" max="360" />
		<radius type="random" min="20" max="300" />
		<position type="random" />
	</properties>
</objectDef>
	

</objectDefinitions>
