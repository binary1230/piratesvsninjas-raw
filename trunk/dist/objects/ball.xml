<?xml version="1.0" encoding="ISO-8859-1"?>

<objectDef name="ball" type="Bounce">
	<animations default="default">
		<animation name="default">
			<frames>
				<frame name="air1.png" duration="1" alpha_enabled="true" />
				<frame name="air2.png" duration="1" alpha_enabled="true" />
				<frame name="air3.png" duration="1" alpha_enabled="true" />
				<frame name="air4.png" duration="1" alpha_enabled="true" />
			</frames>
			<show_in_order />
			<repeat />
		</animation>
	</animations>
	<properties>
		<affectedByGravity />
	</properties>
	<sounds>
		<sound name="ball_hit">sounds/ball_hit.wav</sound>
	</sounds>
</objectDef>
