<?xml version="1.0" encoding="ISO-8859-1"?>

<objectDefinitions>

<objectDef name="amaya" type="Player">
	<animations default="standing">
	
		<animation name="standing">
			<frames>
				<frame name="amaya2/ninjagirl-idle1.png" duration="20" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle7.png" duration="1" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle1.png" duration="30" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle7.png" duration="2" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle1.png" duration="40" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle7.png" duration="1" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle1.png" duration="50" alpha_enabled="true" />
				
				<frame name="amaya2/ninjagirl-idle2.png" duration="1" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle3.png" duration="1" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle4.png" duration="1" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle5.png" duration="1" alpha_enabled="true" />
				<frame name="amaya2/ninjagirl-idle6.png" duration="1" alpha_enabled="true" />
				
			</frames>
			<show_in_order />
			<repeat />
		</animation>
	
		<animation name="walking">
			<frames>
				<frame name="amaya/run1.png" duration="2" />
				<frame name="amaya/run2.png" duration="2" />
				<frame name="amaya/run3.png" duration="2" />
				<frame name="amaya/run4.png" duration="2" />
				<frame name="amaya/run5.png" duration="2" />
				<frame name="amaya/run6.png" duration="2" />
				<frame name="amaya/run7.png" duration="2" />
				<frame name="amaya/run8.png" duration="2" />
				<frame name="amaya/run9.png" duration="2" />
			</frames>
			<show_in_order />
			<repeat />
		</animation>

		<animation name="sliding">
			<frames>
				<frame name="amaya/slide-on-wall-tmpDOM.png" duration="64" />
			</frames>
			<show_in_order />
			<repeat />
		</animation>

		<animation name="jumping">
			<frames>
				<frame name="amaya/jump.png" duration="3" />
			</frames>
			<show_in_order />
			<repeat />
		</animation>
	</animations>
	
	<sounds>
		<sound name="jump">sounds/jump.wav</sound>
	</sounds>
	
	<properties>
		<!--
		<onSkateboard />
		-->

		<affectedByGravity />
		<affectedByInput1 />
		<affectedByFriction />
 		<jumpVelocity>	30.0f	</jumpVelocity>
		<drag>					0.92f	</drag>
		<minVelocity>		4.0f	</minVelocity>
		<mass>					1.0f	</mass>
	</properties>
</objectDef>


</objectDefinitions>
