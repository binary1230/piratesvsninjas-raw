<?xml version="1.0" encoding="ISO-8859-1"?>

<objectDef name="ring" type="Collectable">
	<animations default="default">
		<animation name="default">
			<frames>
				<img name="data/ring1.png" duration="2" />
				<img name="data/ring2.png" duration="2" />
				<img name="data/ring3.png" duration="2" />
				<img name="data/ring4.png" duration="2" />
			</frames>
			<show_in_order />
			<repeat />
		</animation>
	</animations>
	<sounds>
		<sound name="ring">data/sounds/ring.wav</sound>
	</sounds>
	<properties>
		<collectable type="ring" />
	</properties>
</objectDef>
